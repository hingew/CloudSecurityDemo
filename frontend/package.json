{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:ci": "CI=true yarn test -- --coverage",
    "lint": "standard | snazzy",
    "fix": "standard --fix",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "react-select": "^3.1.1",
    "web-vitals": "^0.2.4",
    "react-router-dom": "5.2.0",
    "js-file-download": "0.4.12",
    "react-dropzone": "11.2.4"
  },
  "devDependencies": {
    "@craco/craco": "5.7.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "autoprefixer": "^9.8.6",
    "snazzy": "^9.0.0",
    "postcss": "^7.0.35",
    "standard": "^16.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.1",
    "@tailwindcss/forms" : "0.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "standard": {
    "globals": [
      "fetch",
      "it",
      "expect",
      "beforeEach",
      "afterEach",
      "describe",
      "jest",
      "MouseEvent",
      "KeyboardEvent",
      "sessionStorage",
      "FormData"
    ],
    "ignore": [
      "./*.js"
    ]
  }
}
